{
  "homepage": "https://vemonet.github.io/jsonld-editor",
  "name": "json-ld-editor-react",
  "version": "0.0.1",
  "description": "JSON-LD web editor component for React, with autocomplete based on the loaded ontologies concepts and properties.",
  "author": {
    "name": "Vincent Emonet",
    "email": "vincent.emonet@gmail.com",
    "url": "https://vemonet.github.io"
  },
  "bugs": {
    "url": "https://github.com/vemonet/jsonld-editor/issues"
  },
  "repository": "https://github.com/vemonet/jsonld-editor",
  "license": "MIT",
  "private": false,
  "keywords": [
    "JSON-LD",
    "RDF",
    "editor",
    "react"
  ],
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "source": "src/index",
  "react-native": "src/index",
  "files": [
    "lib/",
    "src/"
  ],
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "dev": "expo web",
    "eject": "expo eject",
    "build": "expo build:web",
    "serve": "serve -s web-build",
    "start": "npm run build && npm run serve",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d web-build",
    "prepare": "bob build"
  },
  "dependencies": {
    "@comunica/actor-init-sparql": "^1.17.0",
    "@solid/react": "^1.10.0",
    "axios": "^0.21.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-labels": "^1.1.0",
    "expo": "~39.0.0",
    "normalizr": "^3.6.0",
    "rdflib": "^2.2.0",
    "react-chartjs-2": "^2.10.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@types/axios": "^0.14.0",
    "@types/chart.js": "^2.9.25",
    "@types/material-ui": "^0.21.8",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-native": "^0.63.25",
    "@types/react-router-dom": "^5.1.6",
    "@types/solid__react": "^1.6.3",
    "expo-cli": "^3.27.13",
    "gh-pages": "^3.1.0",
    "react-native-builder-bob": "^0.18.1",
    "serve": "^11.3.2",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*",
    "react-router-dom": "*",
    "react-router-native": "*",
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@material-ui/lab": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}
