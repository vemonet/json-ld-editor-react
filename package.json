{
  "homepage": "https://vemonet.github.io/jsonld-editor",
  "name": "json-ld-editor-react",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo web",
    "eject": "expo eject",
    "build": "rm -rf dist && tsc --outDir dist",
    "build:bob": "bob build",
    "build:babel": "rm -rf dist && NODE_ENV=production babel ./src/ --extensions .ts --out-dir dist --copy-files",
    "build:web": "expo build:web",
    "dev": "watch 'yarn build:bob' ./src",
    "serve": "serve -s web-build",
    "start": "npm run build && npm run serve",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d web-build"
  },
  "dependencies": {
    "@rjsf/core": "^4.2.0",
    "@rjsf/material-ui": "^4.2.0",
    "axios": "^0.27.2",
    "highlight.js": "^11.5.1",
    "json-form-custom-element": "^1.12.0",
    "n3": "^1.16.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/n3": "^1.10.4",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-native": "^0.67.7",
    "@types/react-router-dom": "^5.1.6",
    "gh-pages": "^4.0.0",
    "react-native-builder-bob": "^0.18.1",
    "serve": "^13.0.2",
    "typescript": "^4.6.4",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-web": "*",
    "react-router-dom": "*",
    "react-router-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "description": "JSON-LD web editor component for React, with autocomplete based on the loaded ontologies concepts and properties.",
  "repository": "https://github.com/vemonet/jsonld-editor",
  "license": "MIT",
  "author": {
    "name": "Vincent Emonet",
    "email": "vincent.emonet@gmail.com",
    "url": "https://vemonet.github.io"
  },
  "bugs": {
    "url": "https://github.com/vemonet/jsonld-editor/issues"
  },
  "keywords": [
    "JSON-LD",
    "RDF",
    "editor",
    "react"
  ],
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "source": "src/index",
  "react-native": "src/index",
  "files": [
    "lib/",
    "src/"
  ]
}
